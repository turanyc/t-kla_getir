-- tablo yapısı dökülüyor incebel_db.kullanici
CREATE TABLE IF NOT EXISTS `kullanici` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `kadi` char(50) CHARACTER SET utf8 COLLATE utf8_turkish_ci NOT NULL,
  `parola` char(50) NOT NULL,
  `yetki` tinyint(4) NOT NULL,
  `email` char(50) NOT NULL,
  `aktif` bit(1) NOT NULL DEFAULT b'1',
  PRIMARY KEY (`id`),
  UNIQUE KEY `k_adi` (`kadi`) USING BTREE
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_turkish_ci;

